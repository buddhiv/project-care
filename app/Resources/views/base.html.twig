<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
        {#Google Fonts#}
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

        {#Meterialize CSS#}
        <link href="{{ asset('/assets/vendor/materialize/dist/css/materialize.min.css') }}" type="text/css"
              rel="stylesheet"/>
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>

<body>
{% block body %}{% endblock %}

{% block javascripts %}
    {#JQuery JS#}
    <script src="{{ asset('/assets/vendor/jquery/dist/jquery.min.js') }}" type="text/javascript"></script>
    {#Materialize JS#}
    <script src="{{ asset('/assets/vendor/materialize/dist/js/materialize.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/vendor/materialize/js/parallax.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/vendor/materialize/js/waves.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/vendor/materialize/js/forms.js') }}" type="text/javascript"></script>
{% endblock %}

</body>
</html>
